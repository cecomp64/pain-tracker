<div class="page-header">
  <%= link_to pain_points_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_pain_point_path(@pain_point), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show pain_point</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @pain_point.user_id %></dd>

  <dt>Magnitude:</dt>
  <dd><%= @pain_point.magnitude %></dd>

  <dt>Notes:</dt>
  <dd><%= @pain_point.notes %></dd>

  <dt>Location:</dt>
  <dd><%= @pain_point.location_id %></dd>

</dl>
